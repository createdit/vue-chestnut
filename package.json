{
  "name": "baizhong",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "dev": "nodemon ./bin/www --dev=true",
    "coverage": "istanbul cover %APPDATA%/npm/node_modules/mocha/bin/_mocha -- -R spec"
  },
  "dependencies": {
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.10",
    "coffeeify": "^1.1.0",
    "debug": "~2.2.0",
    "express": "^4.13.3",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-coffee": "^2.3.1",
    "gulp-copy": "0.0.2",
    "gulp-eslint": "^0.15.0",
    "gulp-filter": "^3.0.1",
    "gulp-if": "^2.0.0",
    "gulp-jade": "^1.1.0",
    "gulp-less": "^3.0.3",
    "gulp-livereload": "^3.8.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-minify-html": "^1.0.4",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rev": "^5.1.0",
    "gulp-ruby-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-usemin": "^0.3.11",
    "gulp-util": "^3.0.6",
    "jade": "~1.11.0",
    "jadeify": "^4.4.0",
    "restify": "^4.0.3",
    "run-sequence": "^1.1.2",
    "stringify": "^3.1.0",
    "utility": "^1.4.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vue": "^1.0.6",
    "vue-router": "^0.7.5",
    "vueify": "^1.1.5",
    "watchify": "^3.3.1",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "gulp-coffeelint": "^0.5.0",
    "tree-kill": "^1.0.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "jadeify"
    ]
  },
  "browserify-shim": {
    "jQuery": "global:jQuery",
    "Vue": "global:Vue",
    "VueAsyncData": "global:VueAsyncData",
    "VueRouter": "global:VueRouter"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "amd": true
    },
    "globals": {
      "module": false,
      "__dirname": false,
      "process": false
    },
    "rules": {
      "strict": [
        2,
        "never"
      ],
      "no-shadow": 0,
      "no-console": 0,
      "camelcase": 0,
      "curly": 2,
      "brace-style": [
        2,
        "1tbs"
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "always"
      ],
      "space-in-brackets": [
        2,
        "never"
      ],
      "space-infix-ops": 2,
      "no-underscore-dangle": 0
    }
  }
}
